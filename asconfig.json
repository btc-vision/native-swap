{
  "targets": {
    "release": {
      "outFile": "build/NativeSwap.wasm"
    }
  },
  "options": {
    "sourceMap": false,
    "optimizeLevel": 3,
    "shrinkLevel": 1,
    "converge": true,
    "noAssert": false,
    "disable": [
      "mutable-globals",
      "sign-extension",
      "nontrapping-f2i",
      "bulk-memory"
    ],
    "runtime": "stub",
    "memoryBase": 0,
    "initialMemory": 1,
    "maximumMemory": 512,
    "exportStart": "start",
    "use": [
      "abort=src/index/abort"
    ],
    "transform": "@btc-vision/opnet-transform"
  }
}
